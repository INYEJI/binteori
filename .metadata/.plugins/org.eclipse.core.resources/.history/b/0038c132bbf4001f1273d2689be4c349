package ch01.ex03;
//여러 개의 연산자가 포함되어 있는 연산식
public class IncreaseDecreaseOperatorExample3 {

	public static void main(String[] args) {
		//증감연산자 - 전위 , 후위 사용O
		
		int a = 1;
		int b = 0;
	
		b = ++a; //우항 먼저 대입, 좌항
		System.out.println("a="+a+",b="+b);
		System.out.printf("a=%d,b=%d\n", a,b);
		
		int c = 1;
		int d = 0;
	
		d = c++; //
		System.out.printf("c=%d, d=%d\n",c,d);
	}

}
